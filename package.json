{
  "name": "configmasher",
  "version": "2.1.1",
  "description": "NodeJS Config aggregator",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "url": "https://github.com/uwu/configmasher.git",
    "type": "git"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf dist && npm run build:types && rollup -c && rimraf dist/types",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir dist/types"
  },
  "keywords": [
    "config",
    "dotenv",
    "config-aggregator",
    "config-parser"
  ],
  "author": "Phorcys",
  "license": "CC0-1.0",
  "dependencies": {
    "defu": "^6.0.0",
    "dotenv": "^16.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "esbuild": "^0.14.51",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.4"
  }
}
